<?xml version="1.0" encoding="UTF-8" ?>
<githubwiki>
    <title>VCard</title>
	<options>
		<verbose>true</verbose>
		<debug>false</debug>
		<norun>false</norun>
		<nophpdoccheck>true</nophpdoccheck>
	</options>
	
	<phpdoc>
		<command>phpdoc30</command>
	</phpdoc>
	
    <paths>
        <output>./wiki/VCard.wiki</output>
        <cache>./wiki/cache</cache>
        <project>./</project>
    </paths>
	
	<source>
		<path>SKien/VCard</path>
	</source>

	<visibilities>
		<visibility>public</visibility>
	</visibilities>            
	
	<ignore-tags>
		<ignore-tag>author</ignore-tag>
		<ignore-tag>version</ignore-tag>
		<ignore-tag>copyright</ignore-tag>
		<ignore-tag>codeCoverageIgnore</ignore-tag>
	</ignore-tags>
</githubwiki>